<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
		<Nullable>enable</Nullable>

		<ContentTargetFolders>.</ContentTargetFolders>
        <Product>Qualia.Umb.ForceLangUrlPrefix</Product>
        <PackageId>Qualia.Umb.ForceLangUrlPrefix</PackageId>
        <Title>Qualia.Umb.ForceLangUrlPrefix</Title>
		<Description>
			An umbraco cms package for multicultural sites, forcing url language prefixes for default language, via permenant redirects.
		</Description>
		<PackageProjectUrl>https://github.com/PagonisTasos/Qualia.Umb.ForceLangUrlPrefix</PackageProjectUrl>
		<PackageTags>umbraco plugin package; url language</PackageTags>
        <RootNamespace>Qualia.Umb.ForceLangUrlPrefix</RootNamespace>
		<Version>1.0.0</Version>
		<Authors>Pagonis Tasos</Authors>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Umbraco.Cms.Web.Website" Version="9.1.2" />
        <PackageReference Include="Umbraco.Cms.Web.BackOffice" Version="9.1.2" />
    </ItemGroup>
</Project>
